<script>
  document.addEventListener('DOMContentLoaded', () => {
    // --- Configuration ---
    // NOTE: Months are 0-indexed in JavaScript (0=Jan, 9=Oct)
    const eventYear = new Date().getFullYear(); // Assumes current year
    const eventDate = new Date(Date.UTC(eventYear, 9, 24, 20, 0, 0)); // October 24th, 8:00 PM
    const eventEnd = new Date(Date.UTC(eventYear, 9, 24, 22, 0, 0)); // October 24th, 10:00 PM
    const timeZone = 'America/Chicago'; // IANA time zone for Central Time

    // Convert UTC dates to the target time zone for accurate comparison
    const eventStartInCT = new Date(eventDate.toLocaleString('en-US', { timeZone }));
    const eventEndInCT = new Date(eventEnd.toLocaleString('en-US', { timeZone }));

    const countdownContainer = document.getElementById('countdown-container');
    const countdownTimerEl = document.getElementById('countdown-timer');

    if (!countdownContainer || !countdownTimerEl) return;

    const createTimeBlock = (value, label) => `
      <div class="flex flex-col items-center">
        <span class="font-sans text-4xl font-bold tracking-tighter text-accent-gold">${value}</span>
        <span class="mt-1 font-body text-xs uppercase tracking-wider text-off-black/70">${label}</span>
      </div>
    `;

    const updateCountdown = () => {
      const nowInCT = new Date(new Date().toLocaleString('en-US', { timeZone }));
      const totalSeconds = (eventStartInCT.getTime() - nowInCT.getTime()) / 1000;

      if (totalSeconds > 0) {
        // --- Before the event ---
        const days = Math.floor(totalSeconds / 3600 / 24);
        const hours = Math.floor(totalSeconds / 3600) % 24;
        const minutes = Math.floor(totalSeconds / 60) % 60;
        const seconds = Math.floor(totalSeconds) % 60;

        countdownTimerEl.innerHTML = `
          ${createTimeBlock(String(days).padStart(2, '0'), 'Days')}
          ${createTimeBlock(String(hours).padStart(2, '0'), 'Hours')}
          ${createTimeBlock(String(minutes).padStart(2, '0'), 'Mins')}
          ${createTimeBlock(String(seconds).padStart(2, '0'), 'Secs')}
        `;
      } else if (nowInCT >= eventStartInCT && nowInCT < eventEndInCT) {
        // --- During the event ---
        countdownContainer.innerHTML = `
          <div class="animate-pulse font-sans text-2xl font-bold tracking-tight text-accent-gold">The event is live!</div>
          <p class="mt-2 font-body text-lg text-off-black">Join us now!</p>
        `;
        clearInterval(countdownInterval);
      } else {
        // --- After the event ---
        countdownContainer.innerHTML = `<div class="font-sans text-xl font-bold text-off-black">See you next month!</div>`;
        clearInterval(countdownInterval);
      }
    };

    updateCountdown();
    const countdownInterval = setInterval(updateCountdown, 1000);
  });
</script>